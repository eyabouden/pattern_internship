{
  "name": "pattern-analysis-dashboard",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:patterns": "node scripts/test-patterns.js",
    "test:performance": "node scripts/test-performance.js",
    "test:validation": "node scripts/test-validation.js",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "analyze": "npm run test:patterns && npm run test:performance && npm run test:validation"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "lucide-react": "^0.263.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "pattern-analysis",
    "data-mining",
    "business-intelligence",
    "sales-analytics",
    "marketing-intelligence",
    "react",
    "typescript",
    "tailwindcss"
  ],
  "author": "Pattern Analysis Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/pattern-analysis-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/pattern-analysis-dashboard/issues"
  },
  "homepage": "https://github.com/your-org/pattern-analysis-dashboard#readme"
}
